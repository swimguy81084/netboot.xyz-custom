    
#!ipxe

# Manjaro Linux Operating System
# https://manjaro.org

goto ${menu}

:manjaro
kernel ${memdisk} iso raw
initrd http://192.168.1.5/manjaro-stable-x86_64.iso
boot

:manjaro_boot
kernel ${memdisk} iso raw
initrd http://192.168.1.5/manjaro-stable-x86_64.iso
boot
goto manjaro_exit

:manjaro_exit
clear menu
exit 0
